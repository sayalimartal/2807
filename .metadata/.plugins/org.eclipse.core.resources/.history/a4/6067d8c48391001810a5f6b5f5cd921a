package com.factorial;

class InvalidInputException extends Exception {

	public InvalidInputException(String message) {
		super(message);
	}
}

public class Factorial {
	
	int getFactorial(int num) throws InvalidInputException{
		
		int fact=1;
		for(int i=1;i<=num;i++)
			fact=fact*i;
		return fact;
	}
}
