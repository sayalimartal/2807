package com.factorial;

class InvalidInputException extends Exception {

	public InvalidInputException(String message) {
		super(message);
	}
}

public class Factorial throws InvalidInputException {
	
	int getFactorial(int num) {
		
		int fact;
		for(int i=1;i<=num;i++)
			fact=fact*i;
		return fact;
	}
}
